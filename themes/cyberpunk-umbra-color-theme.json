	"tokenColors": [
		{
            "name": "Global settings",
			"settings": {
				"background": "#0c0a1f",
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75715E"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "string constant.other.placeholder", "variable.other.constant"],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Language Variable",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#F92672"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"name": "Operator, Punctuation",
			"scope": [
				"keyword.operator",
				"constant.other.color",
				"punctuation"
			],
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag", "meta.tag", "markup.deleted"],
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"name": "Function, Special Method, Method Call",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
                "meta.function-call"
			],
			"settings": {
				"foreground": "#00ff9f"
			}
		},
		{
			"name": "Constants, Numbers",
			"scope": [
				"constant",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "String",
			"scope": ["string", "markup.inserted", "punctuation.definition.string"],
			"settings": {
				"foreground": "#fd5ff0"
			}
		},
		{
			"name": "Class, Type, Support",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"support.class",
				"entity.other.inherited-class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#22a0b6"
			}
		},
		{
			"name": "Attribute",
			"scope": ["entity.other.attribute-name", "meta.object-literal.key"],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FF0000",
				"background": "#F9267233"
			}
		},
		{
			"name": "Markup - Heading",
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#cb0c59",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#fd5ff0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#fd5ff0",
				"fontStyle": "bold"
			}
        },
        {
			"name": "Markup - Inline Code (Backticks)",
			"scope": ["markup.inline.raw.string.markdown", "markup.inline.raw.markdown"],
			"settings": {
				"foreground": "#00ff9f"
			}
		},
		{
			"name": "Markup - Link",
			"scope": ["markup.underline.link", "string.other.link"],
			"settings": {
				"foreground": "#22a0b6",
				"fontStyle": "underline"
			}
		},
        {
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline",
                "foreground": "#22a0b6"
			}
		}
	],
	"semanticHighlighting": true
}
