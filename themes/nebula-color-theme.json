	"tokenColors": [
		{
            "name": "Global settings",
			"settings": {
				"background": "#021513",
				"foreground": "#A8F5E5"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#56948A"
			}
		},
		{
			"name": "Variable, Object key, Other Properties",
			"scope": ["variable", "meta.object-literal.key", "support.type.property-name"],
			"settings": {
				"foreground": "#A8F5E5"
			}
		},
		{
			"name": "Keyword, Storage, Operator, Punctuation",
			"scope": ["keyword", "storage.type", "storage.modifier", "keyword.operator", "punctuation"],
			"settings": {
				"foreground": "#F50057"
			}
		},
		{
			"name": "Function, Method, Tag",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
                "meta.function-call",
				"entity.name.tag",
				"markup.deleted"
			],
			"settings": {
				"foreground": "#39DE8D"
			}
		},
		{
			"name": "Constants, Numbers, Units",
			"scope": [
				"constant",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#FFEA00"
			}
		},
		{
			"name": "String, Attribute Values, Inserted Markup",
			"scope": ["string", "markup.inserted", "punctuation.definition.string"],
			"settings": {
				"foreground": "#11C7FC"
			}
		},
		{
			"name": "Class, Type, Attribute Name",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"support.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0FF6DF"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#FF4081"
			}
		},
		{
			"name": "Markup - Heading",
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#F50057",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#11C7FC",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#11C7FC",
				"fontStyle": "bold"
			}
        },
        {
			"name": "Markup - Inline Code (Backticks)",
			"scope": ["markup.inline.raw.string.markdown", "markup.inline.raw.markdown"],
			"settings": {
				"foreground": "#39DE8D"
			}
		},
        {
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline",
                "foreground": "#7BD3C6"
			}
		}
	],
	"semanticHighlighting": true
}
