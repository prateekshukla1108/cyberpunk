	"tokenColors": [
		{
			"name": "Default text and variables",
			"scope": [
				"variable",
				"meta.definition.variable.js",
				"source",
				"string.unquoted"
			],
			"settings": {
				"foreground": "#D6C1C9"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7b1346"
			}
		},
		{
			"name": "Strings and template literals",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#fffc58"
			}
		},
		{
			"name": "Control flow & storage keywords (if, return, const, let)",
			"scope": [
				"keyword.control",
				"storage.type",
				"keyword.operator.new"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cb0c59"
			}
		},
		{
			"name": "Other keywords and operators",
			"scope": [
				"keyword.operator",
				"keyword.other"
			],
			"settings": {
				"foreground": "#ff547c"
			}
		},
		{
			"name": "Numbers, booleans, null, and other constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.other"
			],
			"settings": {
				"foreground": "#d57bff"
			}
		},
		{
			"name": "Functions, methods, and labels",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call",
				"entity.name.label"
			],
			"settings": {
				"foreground": "#00ff9f"
			}
		},
		{
			"name": "Types, classes, and structs",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.other.inherited-class",
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#22a0b6"
			}
		},
		{
			"name": "Object properties and function parameters",
			"scope": [
				"variable.parameter",
				"meta.object-literal.key",
				"support.type.property-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff4f87"
			}
		},
		{
			"name": "Language variables (this, self, super)",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d57bff"
			}
		},
		{
			"name": "Punctuation (brackets, commas, etc)",
			"scope": [
				"punctuation",
				"meta.brace"
			],
			"settings": {
				"foreground": "#a17e8c"
			}
		},
		{
			"name": "HTML/XML/JSX tags and component names",
			"scope": [
				"entity.name.tag",
				"support.class.component"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff547c"
			}
		},
		{
			"name": "HTML/XML attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00ff9f"
			}
		},
		{
			"name": "CSS selectors, IDs, classes",
			"scope": [
				"meta.selector",
				"entity.name.tag.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#d57bff"
			}
		},
		{
			"name": "JSON keys and CSS property names",
			"scope": [
				"support.type.property-name.json",
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#22a0b6"
			}
		},
		{
			"name": "Markdown: Headings",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cb0c59"
			}
		},
		{
			"name": "Markdown: Links, bold, italic",
			"scope": [
				"markup.underline.link",
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#e03e7c"
			}
		},
		{
			"name": "Markdown: Lists & quotes",
			"scope": [
				"markup.list",
				"markup.quote"
			],
			"settings": {
				"foreground": "#a17e8c"
			}
		},
		{
			"name": "Invalid code",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#cb0c59"
			}
		}
	]
}
