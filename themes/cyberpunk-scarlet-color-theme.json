  "tokenColors": [
    {
      "name": "Default text and variables",
      "scope": [
        "variable",
        "meta.definition.variable.js",
        "source",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#FFD1D1"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7b1346"
      }
    },
    {
      "name": "Strings and template literals",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#fd5ff0"
      }
    },
    {
      "name": "Control flow & storage keywords (if, return, const, let)",
      "scope": [
        "keyword.control",
        "storage.type",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#FF1144"
      }
    },
    {
      "name": "Other keywords and operators",
      "scope": [
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#FF1144"
      }
    },
    {
      "name": "Numbers, booleans, null, and other constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Functions, methods, and labels",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "entity.name.label"
      ],
      "settings": {
        "foreground": "#00ff9f"
      }
    },
    {
      "name": "Types, classes, and structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00BFFF"
      }
    },
    {
      "name": "Object properties and function parameters",
      "scope": [
        "variable.parameter",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff4f87"
      }
    },
    {
      "name": "Language variables (this, self, super)",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF0033"
      }
    },
    {
      "name": "Punctuation (brackets, commas, etc)",
      "scope": [
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#a17e8c"
      }
    },
    {
      "name": "HTML/XML/JSX tags and component names",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#FF1144"
      }
    },
    {
      "name": "HTML/XML attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00ff9f"
      }
    },
    {
      "name": "CSS selectors, IDs, classes",
      "scope": [
        "meta.selector",
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#d57bff"
      }
    },
    {
      "name": "JSON keys and CSS property names",
      "scope": [
        "support.type.property-name.json",
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "name": "Markdown: Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cb0c59"
      }
    },
    {
      "name": "Markdown: Links, bold, italic",
      "scope": [
        "markup.underline.link",
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#e03e7c"
      }
    },
    {
      "name": "Markdown: Lists & quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#a17e8c"
      }
    },
    {
      "name": "Invalid code",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#FF1144"
      }
    }
  ]
}
