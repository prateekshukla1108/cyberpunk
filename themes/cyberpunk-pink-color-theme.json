	"tokenColors": [
		{
			"settings": {
				"foreground": "#E0D0FF",
				"background": "#080A1E"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#706090"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#D0D8FF"
			}
		},
		{
			"name": "Language Variables (this, super, self)",
			"scope": "variable.language",
			"settings": {
				"foreground": "#FF70D0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Colors",
