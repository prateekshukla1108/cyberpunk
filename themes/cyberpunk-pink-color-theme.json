	"tokenColors": [
		{
			"settings": {
				"foreground": "#E0D0FF", // Default token color (bright, slightly purplish white)
				"background": "#080A1E"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#706090" // Muted purple, like Umbra's #6766b3
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#D0D8FF" // Bright, very light blue/lavender
			}
		},
        {
            "name": "Language Variables (this, super, self)",
            "scope": "variable.language",
            "settings": {
                "foreground": "#FF70D0", // Lighter, vibrant pink
                "fontStyle": "italic"
            }
        },
		{
			"name": "Colors", // For CSS color values if specifically scoped
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#D0D8FF"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF2040", // Bright red for invalid
                "fontStyle": "underline"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword", // if, else, for, while, etc.
				"storage.type", // var, let, const, function, class
				"storage.modifier" // static, async, public
			],
			"settings": {
				"foreground": "#D050FF" // Strong, vibrant purple (like Umbra's #d57bff)
			}
		},
		{
			"name": "Operator, Misc Punctuation",
			"scope": [
				"keyword.control", // return, import, export, from, try, catch (often different from other keywords)
				"punctuation", // { } [ ] ( ) ; , . :
				"meta.tag", // Punctuation within tags < > />
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template", // Punctuation for template literals `${}`
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#00C0F0" // Bright Cyan (like Umbra's #00b0ff for operators)
			}
		},
		{
			"name": "Tag Name (HTML, XML)",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#FF40C8" // Electric Pink (like Umbra's #ff5680)
			}
		},
		{
			"name": "Function Name, Method Name",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function", // For function expressions assigned to vars
				"support.function", // Built-in functions
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#40E0D0" // Bright Turquoise/Teal (distinct from general operators)
			}
		},
		{
			"name": "Other Variable, String Link", // e.g. CSS variables
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#00E5B0" // Another shade of bright cyan/greenish-blue
			}
		},
		{
			"name": "Number, Constant, Function Argument, Unit",
			"scope": [
				"constant.numeric",
				"constant.language", // true, false, null
				"support.constant", // Other language-provided constants
				"constant.character", // Character literals
				"constant.escape", // Escape sequences in strings
				"variable.parameter", // Function parameters
				"keyword.other.unit" // CSS units like px, em
			],
			"settings": {
				"foreground": "#FFEA00" // Bright Yellow (like Umbra's #fffc58)
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key", // Object keys
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#60A0FF" // Bright, saturated Blue (like Umbra's #76c1ff)
			}
		},
		{
			"name": "Class Name, Type Name, Namespace",
			"scope": [
				"entity.name", // class names, interface names, type names, modules
				"support.type", // primitive types if not covered by constant.language
				"support.class", // built-in classes
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#00E5B0" // Bright Cyan/Aqua (like Umbra's #00FF9C)
			}
		},
		{
			"name": "CSS Property Name",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#A0B0FF" // Lighter, slightly desaturated blue for CSS properties
			}
		},
		{
			"name": "JS/TS Module Imports/Exports",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js" // Class names in JS often fall here too
			],
			"settings": {
				"foreground": "#FF40C8" // Electric Pink (same as tags for consistency with Umbra)
			}
		},
		{
			"name": "HTML/XML Attribute Name",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E080FF" // Light, vibrant purple (like Umbra's #ee6dff)
			}
		},
		{
			"name": "Inserted", "scope": "markup.inserted",
			"settings": {"foreground": "#80FF80"} // Bright Green
		},
		{
			"name": "Deleted", "scope": "markup.deleted",
			"settings": {"foreground": "#FF6060"} // Bright Red
		},
		{
			"name": "Changed", "scope": "markup.changed",
			"settings": {"foreground": "#FFD060"} // Bright Orange/Yellow
		},
		{
			"name": "Regular Expressions", "scope": "string.regexp",
			"settings": {"foreground": "#40E0D0"} // Turquoise/Teal
		},
		{
			"name": "URL", "scope": ["*url*", "*link*", "*uri*"],
			"settings": {"fontStyle": "underline"}
		},
		{
			"name": "Decorators (@)",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js",
                "meta.decorator entity.name.function",
                "meta.decorator punctuation.decorator",
                "meta.decorator variable.other.readwrite"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D050FF" // Strong Purple (same as keywords)
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": "source.json meta.structure.dictionary.json support.type.property-name.json",
			"settings": {"foreground": "#D050FF"} // Strong Purple
		},
		{
			"name": "JSON Key - Level 1",
			"scope": "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
			"settings": {"foreground": "#FF40C8"} // Electric Pink
		},
		{
			"name": "JSON Key - Level 2",
			"scope": "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
			"settings": {"foreground": "#00C0F0"} // Bright Cyan
		},
		{
			"name": "JSON Key - Level 3+",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {"foreground": "#40E0D0"} // Turquoise/Teal
		},
		{
			"name": "Markdown - Plain",
			"scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
			"settings": {"foreground": "#C0B0E0"}
		},
		{
			"name": "Markdown - Heading",
			"scope": ["markdown.heading", "markup.heading | markup.heading entity.name", "markup.heading.markdown punctuation.definition.heading.markdown"],
			"settings": {"foreground": "#FF40C8", "fontStyle": "bold"}
		},
		{
			"name": "Markup - Italic", "scope": "markup.italic",
			"settings": {"fontStyle": "italic", "foreground": "#D050FF"}
		},
		{
			"name": "Markup - Bold", "scope": ["markup.bold", "markup.bold string"],
			"settings": {"fontStyle": "bold", "foreground": "#D050FF"}
		},
		{
			"name": "Markdown - Link",
			"scope": ["string.other.link.title.markdown", "markup.underline.link.markdown"],
			"settings": {"foreground": "#00C0F0"}
		},
		{
			"name": "Markdown - Code Block",
			"scope": ["markup.inline.raw.string.markdown", "markup.fenced_code.block.markdown"],
			"settings": {"foreground": "#60A0FF"}
		},
		{"scope": "token.info-token", "settings": {"foreground": "#00C0F0"}},
		{"scope": "token.warn-token", "settings": {"foreground": "#FFEA00"}},
		{"scope": "token.error-token", "settings": {"foreground": "#FF3080"}},
		{"scope": "token.debug-token", "settings": {"foreground": "#D050FF"}}
	]
}
