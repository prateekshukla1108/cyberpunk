	"tokenColors": [
		{
            "name": "Global settings",
			"settings": {
				"background": "#10011F",
				"foreground": "#F5C592"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#AB8A67"
			}
		},
		{
			"name": "Variable, Object key, Properties",
			"scope": ["variable", "meta.object-literal.key", "support.type.property-name"],
			"settings": {
				"foreground": "#F5C592"
			}
		},
		{
			"name": "Keyword, Storage, Operator, Punctuation",
			"scope": ["keyword", "storage.type", "storage.modifier", "keyword.operator", "punctuation"],
			"settings": {
				"foreground": "#981357"
			}
		},
		{
			"name": "Function, Method, Tag",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
                "meta.function-call",
				"entity.name.tag",
				"markup.deleted"
			],
			"settings": {
				"foreground": "#50FA7B"
			}
		},
		{
			"name": "Constants, Numbers, Units",
			"scope": [
				"constant",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#FB9B37"
			}
		},
		{
			"name": "String, Attribute Values, Inserted Markup",
			"scope": ["string", "markup.inserted", "punctuation.definition.string"],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Class, Type, Attribute Name",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"support.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#80FFEA"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#FF5555"
			}
		},
		{
			"name": "Markup - Heading",
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#FB9B37",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#FFCB6B",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#FFCB6B",
				"fontStyle": "bold"
			}
        },
		{
			"name": "Markup - Quote",
			"scope": ["markup.quote"],
			"settings": {
				"foreground": "#80FFEA",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup - List",
			"scope": ["markup.list"],
			"settings": {
				"foreground": "#FB9B37"
			}
		},
        {
			"name": "Markup - Inline Code (Backticks)",
			"scope": ["markup.inline.raw.string.markdown", "markup.inline.raw.markdown"],
			"settings": {
				"foreground": "#50FA7B"
			}
		},
		{
			"name": "CSS Class and ID",
			"scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
			"settings": {
				"foreground": "#50FA7B"
			}
		},
		{
			"name": "JSON Property",
			"scope": ["support.type.property-name.json"],
			"settings": {
				"foreground": "#80FFEA"
			}
		},
        {
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline",
                "foreground": "#AB8A67"
			}
		}
	],
	"semanticHighlighting": true
}
