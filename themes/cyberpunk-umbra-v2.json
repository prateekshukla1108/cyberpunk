	"tokenColors": [
		{
            "name": "Global settings",
			"settings": {
				"background": "#0A0E2A",
				"foreground": "#D0EFFF"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variable, Object key, Properties",
			"scope": ["variable", "meta.object-literal.key", "support.type.property-name"],
			"settings": {
				"foreground": "#D0EFFF"
			}
		},
		{
			"name": "Keyword, Storage, Operator",
			"scope": ["keyword", "storage.type", "storage.modifier", "keyword.operator"],
			"settings": {
				"foreground": "#D000F7"
			}
		},
		{
			"name": "Function, Method",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
                "meta.function-call"
			],
			"settings": {
				"foreground": "#00FF9C"
			}
		},
		{
			"name": "Constants, Numbers, Units",
			"scope": [
				"constant",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#FFF02A"
			}
		},
		{
			"name": "String, Attribute Values, Punctuation",
			"scope": ["string", "punctuation.definition.string", "punctuation.definition.tag"],
			"settings": {
				"foreground": "#00B0FF"
			}
		},
		{
			"name": "Class, Type, Tag, Attribute Name",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"support.class",
				"entity.other.inherited-class",
				"entity.name.tag",
				"entity.other.attribute-name",
				"markup.deleted"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00E6A7"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#E91E63"
			}
		},
		{
			"name": "Markup - Heading",
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#D000F7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#00B0FF",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#00B0FF",
				"fontStyle": "bold"
			}
        },
        {
			"name": "Markup - Inline Code (Backticks)",
			"scope": ["markup.inline.raw.string.markdown", "markup.inline.raw.markdown"],
			"settings": {
				"foreground": "#00FF9C"
			}
		},
        {
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline",
                "foreground": "#78909C"
			}
		}
	],
	"semanticHighlighting": true
}
